{
    "name": "kalebalebachew/hasab-laravel",
    "description": "A Laravel SDK for Hasab AI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kaleb Alebachew",
            "email": "kalebalebachew4@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "autoload": {
        "psr-4": {
            "Hasab\\": "src/"
        }
    },
    "autoload-dev": {
    "psr-4": {
        "Hasab\\Tests\\": "tests/"
    }
},
    "extra": {
        "laravel": {
            "providers": [
                "Hasab\\HasabServiceProvider"
            ],
            "aliases": {
                "Hasab": "Hasab\\Facades\\Hasab"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "phpunit/phpunit": "^11.5"
    },
    "scripts": {
    "test": "vendor/bin/phpunit"
}
}
